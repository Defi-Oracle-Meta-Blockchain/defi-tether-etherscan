# Environment variables for DeFi Oracle Meta Mainnet and Ethereum Mainnet integration

# Ethereum Mainnet
ETHEREUM_MAINNET_URL=https://mainnet.infura.io/v3/YOUR_INFURA_PROJECT_ID
ETHEREUM_PRIVATE_KEY=YOUR_ETHEREUM_PRIVATE_KEY

# DeFi Oracle Meta Mainnet
DEFI_ORACLE_META_MAINNET_URL=https://defi-oracle-meta-mainnet.io
DEFI_ORACLE_META_PRIVATE_KEY=YOUR_DEFI_ORACLE_META_PRIVATE_KEY

# Etherscan API Key
ETHERSCAN_API_KEY=YOUR_ETHERSCAN_API_KEY

# Web3j Configuration
WEB3J_CLIENT_URL=https://mainnet.infura.io/v3/YOUR_INFURA_PROJECT_ID
WEB3J_PRIVATE_KEY=YOUR_WEB3J_PRIVATE_KEY

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=defi_oracle_meta
DB_USER=your_db_user
DB_PASSWORD=your_db_password

# Logging Configuration
LOG_LEVEL=info
LOG_FILE_PATH=./logs/app.log

# Security Configuration
JWT_SECRET=your_jwt_secret
ENCRYPTION_KEY=your_encryption_key

# API Keys for External Services
CHAINLINK_API_KEY=your_chainlink_api_key
FORTA_API_KEY=your_forta_api_key

# Miscellaneous
NODE_ENV=production
PORT=3000
